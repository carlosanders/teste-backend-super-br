{
	"name": "supp-core/supp-administrativo-backend",
	"description": "Sistema Único de Procuradorias Públicas",
	"version": "1.11.0",
	"type": "project",
	"keywords": [
		"Symfony",
		"Flex",
		"API"
	],
	"homepage": "https://www.agu.gov.br",
	"license": "MIT",
	"authors": [
		{
			"name": "Advocacia-Geral da União",
			"email": "supp@agu.gov.br",
			"homepage": "https://www.agu.gov.br",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://www.agu.gov.br"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^8.1.0",
		"ext-bcmath": "*",
		"ext-curl": "*",
		"ext-dom": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-json": "*",
		"ext-ldap": "*",
		"ext-mbstring": "*",
		"ext-openssl": "*",
		"ext-redis": "*",
		"ext-soap": "*",
		"ext-zip": "*",
		"ext-zlib": "*",
		"ext-ssh2": "*",
		"ass/xmlsecurity": "1.1.1",
		"chrisguitarguy/request-id-bundle": "5.1.0",
		"dms/dms-filter-bundle": "4.0.3",
		"doctrine/annotations": "1.13.3",
		"doctrine/dbal": "3.5.2",
		"doctrine/doctrine-bundle": "2.8.0",
		"doctrine/doctrine-fixtures-bundle": "3.4.2",
		"doctrine/doctrine-migrations-bundle": "3.2.2",
		"doctrine/orm": "2.14.0",
		"doctrine/persistence": "3.1.2",
		"dragonmantank/cron-expression": "^3.3",
		"elasticsearch/elasticsearch": "7.17.0",
		"endroid/qr-code-bundle": "4.2.0",
		"jms/serializer-bundle": "5.0.2",
		"knplabs/knp-gaufrette-bundle": "0.8.0",
		"knplabs/knp-paginator-bundle": "6.0.1",
		"knplabs/knp-snappy-bundle": "1.9.0",
		"lexik/jwt-authentication-bundle": "2.16.0",
		"nelmio/api-doc-bundle": "4.10.2",
		"nelmio/cors-bundle": "2.2.0",
		"nesbot/carbon": "^2.59",
		"ongr/elasticsearch-bundle": "dev-master",
		"ongr/elasticsearch-dsl": "7.2.x-dev",
		"phpdocumentor/reflection-docblock": "5.3.0",
		"phpoffice/phpspreadsheet": "1.26.0",
		"phpseclib/phpseclib": "3.0.18",
		"piwik/device-detector": "6.0.4",
		"ramsey/uuid": "4.7.0",
		"sensio/framework-extra-bundle": "6.2.9",
		"snc/redis-bundle": "4.4.2",
		"stof/doctrine-extensions-bundle": "1.7.1",
		"swaggest/json-schema": "v0.12.41",
		"symfony/acl-bundle": "2.3.0",
		"symfony/amqp-messenger": "6.2.2",
		"symfony/asset": "6.2.0",
		"symfony/console": "6.2.3",
		"symfony/dotenv": "6.2.0",
		"symfony/expression-language": "6.2.2",
		"symfony/flex": "2.2.4",
		"symfony/form": "6.2.3",
		"symfony/framework-bundle": "6.2.3",
		"symfony/ldap": "6.2.0",
		"symfony/lock": "6.2.0",
		"symfony/mailer": "6.2.2",
		"symfony/mercure": "0.6.2",
		"symfony/mercure-bundle": "0.3.5",
		"symfony/messenger": "6.2.2",
		"symfony/mime": "6.2.2",
		"symfony/monolog-bundle": "3.8.0",
		"symfony/process": "6.2.0",
		"symfony/property-access": "6.2.3",
		"symfony/property-info": "6.2.3",
		"symfony/proxy-manager-bridge": "6.2.0",
		"symfony/rate-limiter": "6.2.2",
		"symfony/runtime": "6.2.0",
		"symfony/security-bundle": "^6.2.3",
		"symfony/serializer": "6.2.3",
		"symfony/templating": "6.2.0",
		"symfony/translation": "6.2.3",
		"symfony/twig-bundle": "6.2.3",
		"symfony/validator": "6.2.3",
		"symfony/workflow": "6.2.3",
		"symfony/yaml": "6.2.2",
		"twig/intl-extra": "3.5.0",
		"webklex/php-imap": "4.1.2",
		"liip/monitor-bundle": "2.19.0"
	},
	"require-dev": {
		"bamarni/composer-bin-plugin": "1.8.2",
		"symfony/browser-kit": "6.2.0",
		"symfony/debug-bundle": "6.2.1",
		"symfony/maker-bundle": "1.39.0",
		"symfony/requirements-checker": "2.0.1",
		"symfony/stopwatch": "6.2.0",
		"symfony/var-dumper": "6.2.3",
		"symfony/web-profiler-bundle": "6.2.3"
	},
	"config": {
		"platform": {
			"php": "8.1.0"
		},
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"allow-plugins": true
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/progmancod/dms-filter-bundle"
		},
		{
			"type": "git",
			"url": "https://github.com/progmancod/ElasticsearchDSL"
		},
		{
			"type": "git",
			"url": "https://github.com/progmancod/ElasticsearchBundle"
		},
		{
			"type": "git",
			"url": "https://github.com/maurobn/PackageBase"
		}
	],
	"autoload": {
		"psr-4": {
			"SuppCore\\AdministrativoBackend\\": "src/"
		},
		"classmap": [],
		"exclude-from-classmap": []
	},
	"autoload-dev": {
		"psr-4": {
			"SuppCore\\AdministrativoBackend\\Tests\\": "tests/",
			"PHPUnit\\": "vendor-bin/phpunit/vendor/phpunit/phpunit/src/",
			"PHPUnit\\Framework\\MockObject\\": "vendor-bin/phpunit/vendor/phpunit/phpunit/src/Framework/MockObject",
			"SebastianBergmann\\FinderFacade\\": "vendor-bin/phpunit/vendor/sebastian/finder-facade/src",
			"Symfony\\Component\\BrowserKit\\": "vendor-bin/phpunit/vendor/symfony/browser-kit",
			"Symfony\\Component\\DomCrawler\\": "vendor-bin/phpunit/vendor/symfony/dom-crawler",
			"Symfony\\Bridge\\PhpUnit\\": "vendor-bin/phpunit/vendor/symfony/phpunit-bridge",
			"TheSeer\\fDOM\\": "vendor-bin/phpunit/vendor/theseer/fdomdocument/src",
			"Liuggio\\Fastest\\": [
				"vendor-bin/phpunit/vendor/liuggio/fastest/src",
				"vendor-bin/phpunit/vendor/liuggio/fastest/adapters"
			],
			"Symplify\\CodingStandard\\": "vendor-bin/ecs/vendor/symplify/easy-coding-standard/vendor/symplify/coding-standard/src",
			"Symplify\\RuleDocGenerator\\": "vendor-bin/ecs/vendor/symplify/easy-coding-standard/vendor/symplify/rule-doc-generator-contracts/src",
			"PHPStan\\PhpDoc\\PHPUnit\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-phpunit/src/PhpDoc/PHPUnit",
			"PHPStan\\Rules\\PHPUnit\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-phpunit/src/Rules/PHPUnit",
			"PHPStan\\Symfony\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-symfony/src/Symfony",
			"PHPStan\\Type\\PHPUnit\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-phpunit/src/Type/PHPUnit",
			"PHPStan\\Type\\Symfony\\": "vendor-bin/phpstan/vendor/phpstan/phpstan-symfony/src/Type/Symfony",
			"Rector\\Core\\": "vendor-bin/rector/vendor/rector/rector/src",
			"Rector\\Php74\\": "vendor-bin/rector/vendor/rector/rector/rules/Php74",
			"Rector\\Php80\\": "vendor-bin/rectorr/vendor/rector/rector/rules/Php80",
			"Rector\\Set\\": "vendor-bin/rector/vendor/rector/rector/packages/Set"
		}
	},
	"replace": {
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-mbstring": "*",
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php81": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"cache:warmup": "symfony-cmd",
			"assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"requirements-checker": "script"
		},
		"post-install-cmd": [
			"rm -rf src/DataFixtures/AppFixtures.php",
			"if test -d vendor/symfony/requirements-checker; then ./vendor/bin/requirements-checker; fi",
			"if test -d vendor/bamarni/composer-bin-plugin; then composer bin all install --optimize-autoloader; fi",
			"@auto-scripts",
			"@composer dump-autoload"
		],
		"post-update-cmd": [
			"rm -rf src/DataFixtures/AppFixtures.php",
			"if test -d vendor/symfony/requirements-checker; then ./vendor/bin/requirements-checker; fi",
			"if test -d vendor/bamarni/composer-bin-plugin; then composer bin all update --optimize-autoloader; fi",
			"@auto-scripts",
			"@composer dump-autoload"
		]
	},
	"conflict": {
		"symfony/symfony": "*",
		"symfony/twig-bundle": "<3.3",
		"symfony/debug": "<3.3"
	},
	"extra": {
		"allow-contrib": "true",
		"projectTitle": "Sistema Único de Procuradorias Públicas",
		"bamarni-bin": {
			"bin-links": true,
			"forward-command": true,
			"target-directory": "vendor-bin"
		}
	}
}
